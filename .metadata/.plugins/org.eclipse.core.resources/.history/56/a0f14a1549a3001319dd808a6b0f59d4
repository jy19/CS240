			if(current.equals(inputword)){
				continue;
			}
			else if(current.length() > inputword.length()){
				continue;
			}
			else{
				
			}
		}

	}
	
	public static int[] createArray(String word){
		int [] countArray = new int [26];
		for(int i = 0; i < word.length(); i++){
			countArray[word.charAt(i) - 'a']++;
		}
		return countArray;
	}
	
